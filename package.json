{
  "name": "MKDocs",
  "version": "1.0.2",
  "description": "Node tooling for use in GitHub Actions.",
  "keywords": [
    "mkdocs"
  ],
  "license": "MIT",
  "author": "Markus Kohlmeyer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RootService/MKDocs.git"
  },
  "homepage": "https://github.com/RootService/MKDocs#readme",
  "bugs": {
    "url": "https://github.com/RootService/MKDocs/issues"
  },
  "scripts": {
    "changelog": "conventional-changelog -p conventionalcommits --config ./build/changelog-generator/index.cjs --infile changelog.md --same-file",
    "changelog:version": "conventional-changelog [patch|minor|major] -p conventionalcommits --infile CHANGELOG.md --same-file && git add CHANGELOG.md",
    "lint:all": "npm run lint:prettier && npm run lint:eslint && npm run lint:stylelint && npm run lint:html && npm run lint:markdown && npm run lint:commit",
    "lint:prettier": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,json,md,markdown,yml,yaml,html,htm,css}\" --config project.config.cjs",
    "lint:eslint": "eslint . --ext .js,.cjs,.mjs,.ts,.tsx,.json --max-warnings=0 --config project.config.cjs",
    "lint:stylelint": "stylelint \"**/*.{css,html,htm,j2,jinja,md,markdown}\" --config project.config.cjs",
    "lint:html": "linthtml \"**/*.{html,htm,j2,jinja}\" --config project.config.cjs",
    "lint:markdown": "markdownlint-cli2 \"**/*.{md,markdown}\" --config project.config.cjs",
    "lint:commit": "commitlint --from=HEAD~1 --to=HEAD --config project.config.cjs",
    "format:all": "npm run format:prettier && npm run format:eslint && npm run format:stylelint && npm run format:html && npm run format:markdown && npm run format:postcss",
    "format:prettier": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,json,md,markdown,yml,yaml,html,htm,css}\" --config project.config.cjs",
    "format:eslint": "eslint . --ext .js,.cjs,.mjs,.ts,.tsx,.json --max-warnings=0 --config project.config.cjs --fix",
    "format:stylelint": "stylelint \"**/*.{css,html,htm,j2,jinja,md,markdown}\" --config project.config.cjs --fix",
    "format:html": "linthtml \"**/*.{html,htm,j2,jinja}\" --config project.config.cjs --fix",
    "format:markdown": "markdownlint-cli2 \"**/*.{md,markdown}\" --config project.config.cjs --fix",
    "format:postcss": "postcss \"src/**/*.{css}\" --config project.config.cjs -d dist/"
  },
  "dependencies": {
    "@actions/artifact": "^2.3.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@lhci/cli": "^0.15.1",
    "@linthtml/linthtml": "^0.10.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@stylistic/stylelint-config": "^3.0.1",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "autoprefixer": "^10.4.21",
    "browserslist": "^4.26.0",
    "browserslist-config-baseline": "^0.5.0",
    "clipboard": "^2.0.11",
    "conventional-changelog": "^7.1.1",
    "cssnano": "^7.1.1",
    "escape-html": "^1.0.3",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "markdownlint-cli2": "^0.18.1",
    "npm-check-updates": "^18.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-dir-pseudo-class": "^9.0.1",
    "postcss-inline-svg": "^6.0.0",
    "postcss-logical": "^8.1.0",
    "postcss-pseudo-is": "^0.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-jinja-template": "^2.1.0",
    "puppeteer": "^24.21.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "semantic-release": "^24.2.8",
    "stylelint": "^16.24.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^7.3.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "svgo": "^4.0.0",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "sharp-cli": "^5.2.0"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=20",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "version": ">=10",
      "onFail": "error"
    }
  },
  "private": true,
  "browserslist": [
    "> 0.2% or last 0.5 years and not dead and fully supports es6-module",
    "maintained node versions",
    "extends browserslist-config-baseline"
  ],
  "browserslist-config-baseline": {
    "includeDownstreamBrowsers": true
  },
  "type": "commonjs",
  "resolutions": {
    "puppeteer/**/devtools-protocol": "0.0.1507524",
    "puppeteer-core/**/devtools-protocol": "0.0.1507524"
  }
}
